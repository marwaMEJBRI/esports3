

<div class="container">   
<%= form_with model: @roster do |f| %>   
        
  
        <div>   
             <label>Name </label>   
             <%= f.text_field :name %>   
        </div>   
        <div>   
      <div>
      <label> team_id</label>
      <%= f.text_field :team_id %>
       </div>
        <div>
      <label> user_id</label>
      <%= f.text_field :user_id %>
       </div>
       
   
      <div>   
        <%= link_to 'Back', { controller: 'rosters', action: 'index'} %>   
        <%= f.submit 'Create roster' ,{ controller: 'rosters', action: 'create'} %>   
      </div>   
  <% end %> 
</div>  


 
